{
    "Name": "Stockholm",
    "Version": 1.0,
    "Author": "Slideas",
    "Description": "Minimalistic and clean theme",
    "Css": "stockholm-theme.css",
    "Palets": [
        {
            "Name": "Default",
            "Css": "default-palet.css",
            "Dark1": "#000000",
            "Light1": "#ffffff",
            "Dark2": "#666666",
            "Light2": "#dddddd",
            "Accent1": "#DD3556",
            "Accent2": "#E36151",
            "Accent3": "#B7ACA1"
        }
    ],
    "Layouts": [
        {
            "Name": "Default",
            "Group": "Basic",
            "Classes": "default",
            "Dynamic": false,
            "DynamicPart": "",
            "Example": "Layout: Default\n\n# Title\n\nWith some **content**\n\n",
            "FixedParts": 1,
            "Template": "<div class = \"innercontent\" data-slide-fit=\"fit\">{@{content}@}</div><div class=\"slidenumber\">{@{FOOTER}@}</div>",
            "ThumbnailTemplate": "<div class = \"innercontent\" data-slide-fit=\"fit\"><h1>Title</h1> And content...</div><div class=\"slidenumber\">1</div>   >",
        },
        {
            "Name": "Title",
            "Group": "Basic",
            "Classes": "title aligncenter",
            "Dynamic": false,
            "DynamicPart": "",
            "Example": "Layout: Title\n\n# Title\n## Level 2 title\n\n",
            "FixedParts": 1,
            "Template": " <div class = \"innercontent inverted\" data-slide-fit=\"fit\">{@{content}@}</div> ",
            "ThumbnailTemplate": " <div class = \"innercontent inverted\" data-slide-fit=\"fit\"><h1>Title</h1><h2>SubTitle</h2></div>     ",
        },
        {
            "Name": "HeaderAndColumns",
            "Group": "Basic",
            "Classes": "titleandcolumns default",
            "Dynamic": true,
            "Example": "Layout: HeaderAndColumns\n\n# Title\n+++\n### Column1\n+++\n### Column2\n+++\n### Column3\n\n",
            "FixedParts": 1,
            "DynamicPart": "<div class=\"column txtfit\"  data-slide-fit=\"fit\"> {@{content}@} </div>",
            "Template": "<div class = \"innercontent\"><div class=\"titleframe\"> {@{content}@} </div> <div class=\"grid\">{@{dynamic}@}</div></div><div class=\"slidenumber\">{@{FOOTER}@}</div>",
            "ThumbnailTemplate": "<div class = \"innercontent\"><div class=\"titleframe\"> <h1>Title</h1 </div> <div class=\"grid\"><div class=\"column txtfit\"  data-slide-fit=\"fit\"> <h2>Column 1</h2> </div><div class=\"column txtfit\"  data-slide-fit=\"fit\"> <h2>Column 2</h2> </div></div></div><div class=\"slidenumber\">1</div>",
        },
        {
            "Name": "SectionTitle",
            "Group": "Basic",
            "Classes": "sectiontitle",
            "Example": "Layout: SectionTitle\n\n# Title\n## Level 2 title\n\n",
            "Dynamic": false,
            "DynamicPart": "",
            "FixedParts": 1,
            "Template": " <div class = \"innercontent\" data-slide-fit=\"fit\">{@{content}@}</div> ",
            "ThumbnailTemplate": " <div class = \"innercontent\" data-slide-fit=\"fit\"><h1>Title</h1><h2>SubTitle</h2></div>     ",
        },
        {
            "Name": "Split",
            "Group": "Basic",
            "Classes": "split",
            "Dynamic": false,
            "DynamicPart": "",
            "Example": "Layout: Split\n\n![](https://slideas.app/img/photos-background.jpg)\n+++\n# Right part\nWith some **content**\n\n",
            "FixedParts": 1,
            "Template": "<div class = \"innercontent\"><div class=\"grid\"><div class=\"column inverted colored txtfit\"  data-slide-fit=\"fit\"> {@{content}@} </div><div class=\"column txtfit \"  data-slide-fit=\"fit\">{@{content}@} </div></div></div><div class=\"slidenumber\">{@{FOOTER}@}</div>",
            "ThumbnailTemplate": "<div class = \"innercontent\"><div class=\"grid\"><div class=\"column inverted colored txtfit\"  data-slide-fit=\"fit\"> <h1>Column 1</h1>  </div><div class=\"column txtfit inverted \"  data-slide-fit=\"fit\"><h1>Column 2</h1>  </div></div></div><div class=\"slidenumber\">1</div>",
        },
        {
            "Name": "KeyPoint",
            "Group": "Basic",
            "Classes": "keypoint",
            "Dynamic": false,
            "DynamicPart": "",
            "FixedParts": 1,
            "Example": "Layout: KeyPoint\n\n# Title\n## Level 2 title\n\n",
            "Template": " <div class = \"innercontent inverted\" data-slide-fit=\"fit\">{@{content}@}</div> ",
            "ThumbnailTemplate": " <div class = \"innercontent inverted\" data-slide-fit=\"fit\"><h1>Title</h1><h2>SubTitle</h2></div>     ",

        }
    ]
}
