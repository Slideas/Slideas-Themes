{
    "Name": "Reveal",
    "Version": 1.0,
    "Author": "Reveal.js",
    "Description": "Port of the Reveal.js Themes. Includes 3 different palettes and a single all-purposes layout.",
    "Css": "reveal-theme.css",
    "Palets": [
        {
            "Name": "Black",
            "Css": "black-palet.css",
            "Dark1": "#222223",
            "Dark2": "#666666",
            "Light1": "#ffffff",
            "Light2": "#eeeeee",
            "Accent1": "#2B5ADC",
            "Accent2": "#c63ebd",
            "Accent3": "#ff4b91",
            "Accent4": "#ff8267",
            "Accent5": "#ffc053",
            "Accent6": "#f9f871"
        },
        {
            "Name": "White",
            "Css": "white-palet.css",
            "Dark1": "#222223",
            "Dark2": "#666666",
            "Light1": "#ffffff",
            "Light2": "#eeeeee",
            "Accent1": "ff6188",
            "Accent2": "#fc9867",
            "Accent3": "#ffd866",
            "Accent4": "#a9dc76",
            "Accent5": "#78dce8",
            "Accent6": "#ab9df2"
        },
        {
            "Name": "Serif",
            "Css": "serif-palet.css",
            "Dark1": "#222223",
            "Dark2": "#666666",
            "Light1": "#ffffff",
            "Light2": "#eeeeee",
            "Accent1": "ff6188",
            "Accent2": "#fc9867",
            "Accent3": "#ffd866",
            "Accent4": "#a9dc76",
            "Accent5": "#78dce8",
            "Accent6": "#ab9df2"
        }
    ],
    "Layouts": [
        {
            "Name": "Default",
            "Group": "Basic",
            "Classes": "default",
            "Dynamic": false,
            "DynamicPart": "",
            "Example": "Layout: Default\n\n# Title\n\nWith some **content**\n\n",
            "FixedParts": 1,
            "Template": "<div class = \"innercontent\" data-slide-fit=\"fit\">{@{content}@}<div class=\"footnotes\">{@{footnotes}@}</div></div><div class=\"slidenumber\">{@{FOOTER}@}</div>",
            "ThumbnailTemplate": "<div class = \"innercontent\" data-slide-fit=\"fit\"><h1>Title</h1> And content...</div><div class=\"slidenumber\">1</div>   >",
        }
    ]
}
